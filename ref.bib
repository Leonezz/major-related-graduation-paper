  author = {Joshi, Jetendra and Reddy, Joshitha and Reddy, Praneeth and Agarwal, Akshay and Agarwal, Rahul and Bagga, Amrit and Bhargava, Abhinandan},
  year = {2016},
  month = aug,
  pages = {70--75},
  publisher = {{IEEE}},
  address = {{Phuket, Thailand}},
  doi = {10.1109/ICED.2016.7804609},
  abstract = {健康的环境是一个健康和幸福的社区所必需的。 由于雇用人员定期检查和清空装满的垃圾桶的古老系统，这个过程很容易出现人为错误和疏忽。此外，由于不同地区的垃圾桶使用频率不同，基于时间缝隙的例行检查是低效的，因为一个垃圾桶可能很早就被填满了，可能需要立即注意，或者可能在很长一段时间内都不需要例行检查。这使得目前的系统资源昂贵而无效，因为溢出的、发臭的垃圾桶成为更多的问题而不是解决方案。 在本文中，我们提出了一个关于智能垃圾桶的解决方案，它是一个整合了物联网和无线传感器网络理念的垃圾桶网络。 我们还提出了基于堆栈式前端方法的智能垃圾箱网络的概念，将无线传感器网络与云计算相结合，并讨论了如何将决策森林回归等机器学习技术应用于系统所利用的传感器数据，以获得有用的见解，从而提高垃圾箱监测的效率。},
  file = {D\:\\毕设\\论文\\refs\\joshi2016.pdf},
  isbn = {978-1-5090-2160-4},
  keywords = {云计算,国外,无线传感器网络,智能垃圾桶,桶满提醒}
}


@phdthesis{zhuying2019,
  title = {{智能垃圾桶的设计与研究}},
  author = {朱, 莹},
  year = {2019},
  month = dec,
  abstract = {近年来，随着经济社会发展、城市规模扩大，生活垃圾的产生量不断增加。 垃圾桶作为生活中随处可见的环卫设施，随着垃圾分类的到来一夜之间集万千关 注于一身。现如今快速发展的时代，不仅分类垃圾桶正在普及，智能垃圾桶在很 多城市也能看到，很大程度地方便了环卫工人的清理与相关工作者的管理，其中 蕴含的管理系统与专业技术要求比较严格也比较高。当然，任何高科技产品的结 果都要回归产品本身的实用性，怎样用最低的价格开发出最实用的产品才是研究 的重点。  本设计以单片机 STM32F103RCT6 为智能控制中心，结合垃圾量检测模块， 称重检测模块，温度检测模块、GPS 定位模块，语音提示模块及其外围辅助电路， 构成了一个多种检测功能为一体的垃圾管理系统。  通过系统控制算法设计和数据、信号处理，本系统实现了智能检测垃圾重量、 垃圾容量、桶内温度，并做对应语音提示，当检测到的信号超出自身的设定值时， 将主动发送数据给服务器，服务器根据通信协议解析数据，并将数据持久化处理， 并推送相关信息到保洁员手机中。基于系统低功耗设计，根据电量多少，系统智 能切换工作模式，实现节省电能目的，且能量全部来自新能源供电系统。  本设计能实现 GPS 定位与数据上传、下载功能，并将数据按照一定时间或事 件触发上传到服务器。通过测试每个模块的程序是否有误、测试模块之间的接口 是否正确、测试整个软件系统是否满足设计功能和性能的要求，保证了产品的可 靠性与稳定性。},
  file = {F\:\\ZeteroData\\storage\\CD77V84Y\\智能垃圾桶的设计与研究_朱莹.caj},
  language = {中文},
  school = {中国矿业大学},
  type = {{工程硕士专业学位论文}}
}

